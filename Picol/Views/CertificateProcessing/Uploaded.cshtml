
@{
    ViewBag.Title = "Uploaded";
}


@{
    ViewBag.Title = "List uploaded certificates";
}

<div id="CertificateGrid" style="width: 100%; height: 500px; float:left;"></div>
<div class="footer">
    <span id="Documentation"><a href="#" target="_blank">Documentation</a></span> |
    <button type="button" id="ExportCertificateGrid">Export</button>
    <span id="CertificateCounter" style="float:right;">Count</span>
</div>

@section styles
{
    @Styles.Render("~/Content/slickgrid")
}

@section scripts
{
    @Scripts.Render("~/bundles/SlickgridFilter")
    @Scripts.Render("~/bundles/filesaver")
    @Scripts.Render("~/bundles/slickgrid")
    @Scripts.Render("~/bundles/Picol_CertificateProcessing_Uploaded")
}