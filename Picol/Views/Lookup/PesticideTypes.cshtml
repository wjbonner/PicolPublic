
@{
    ViewBag.Title = "PesticideType";
}

<div id="PesticideTypeGrid" style="width: 100%; height: 500px; float:left;"></div>
<div id="PesticideTypeFooter" class="footer">
    <span id="Documentation"><a href="#" target="_blank">Documentation</a></span>
    <button type="button" id="ExportPesticideTypeGrid">Export</button>
    <span id="PesticideTypeCounter" style="float:right;">Count</span>
</div>

@section styles
{
    @Styles.Render("~/Content/slickgrid")
}

@section scripts
{
    @Scripts.Render("~/bundles/SlickgridFilter")
    @Scripts.Render("~/bundles/filesaver")
    @Scripts.Render("~/bundles/slickgrid")
    @Scripts.Render("~/bundles/Picol_Lookup_PesticideTypes")
}