
@{
    ViewBag.Title = "Registrant";
}

<div id="RegistrantGrid" style="width: 100%; height: 500px; float:left;"></div>
<div id="RegistrantFooter" class="footer">
    <span id="Documentation"><a href="#" target="_blank">Documentation</a></span>
    <button type="button" id="ExportRegistrantGrid">Export</button>
    <span id="RegistrantCounter" style="float:right;">Count</span>
</div>

@section styles
{
    @Styles.Render("~/Content/slickgrid")
}

@section scripts
{
    @Scripts.Render("~/bundles/SlickgridFilter")
    @Scripts.Render("~/bundles/filesaver")
    @Scripts.Render("~/bundles/slickgrid")
    @Scripts.Render("~/bundles/Picol_Lookup_Registrants")
}