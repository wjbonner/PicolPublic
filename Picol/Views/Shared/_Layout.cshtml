<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-svg lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-svg lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-svg lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title | Washington State University</title>

    <!-- FAVICON -->
    <link rel="shortcut icon" href="//repo.wsu.edu/spine/1/favicon.ico" />

    <!-- STYLESHEETS -->
    <link href="//repo.wsu.edu/spine/1/spine.min.css" rel="stylesheet" type="text/css" />
    @Styles.Render("~/Content/base")
    @RenderSection("styles", required: false)

    <!-- RESPOND -->
    <meta name="viewport" content="width=device-width, user-scalable=yes">

    <!-- SCRIPTS -->
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/base")
    <script src="//repo.wsu.edu/spine/1/spine.js"></script>
    <script type="text/javascript">
        $.CFG = {
            contextPath: '@Url.Content("~")'
        };

        if ($.CFG.contextPath == '/') {
            $.CFG.contextPath = '';
        } else {
            $.CFG.contextPath.slice(0, -1);
        }
    </script>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-124281315-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-124281315-1');
    </script>

    @RenderSection("scripts", required: false)

    <!-- COMPATIBILITY -->
    <!--[if lt IE 9]><script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <noscript>
        <style>
            #spine .spine-sitenav ul ul li {
                display: block !important;
            }
        </style>
    </noscript>

    <!-- DOCS -->
    <link type="text/plain" rel="author" href="//repo.wsu.edu/spine/1/authors.txt">
    <link type="text/html" rel="help" href="//brand.wsu.edu">

    <!-- ANALYTICS -->
    <!-- Your analytics code here -->
</head>
<body>
    <!-- WRAPPING -->
    <div id="jacket">
        <div id="binder" class="fluid">
            <!-- CONTENT -->
            <main>
                <!-- Responsive Sections -->
                <section class="row sidebar">
                    <div class="column one">
                        <article>
                            <div id="MessageBar" class="MessageBar">
                                <span id="BarMessage" class="BarMessage">The database is currently updating, check back in 15 minutes for the latest data.</span>
                            </div>
                            <div id="OverlayDisplay" class="OverlayDisplay">
                                <div id="OverlaySpinner" class="OverlaySpinner"></div>
                                <span id="OverlayMessage" class="OverlayMessage"></span>
                            </div>
                            <div id="Overlay" class="Overlay" style="display:none;"></div>

                            @RenderBody()
                        </article>
                    </div>
                </section>
            </main>

            <!-- SPINE -->
            <div id="spine" class="spine-column shelved bleed">
                <div id="glue" class="spine-glue">
                    <header class="spine-header">
                        <a href="http://www.wsu.edu/" id="wsu-signature" class="spine-signature">Washington State University</a>
                    </header>

                    <!-- ACTIONS -->
                    <section id="wsu-actions" class="spine-actions">

                        <!-- Tabs -->
                        <ul id="wsu-actions-tabs" class="spine-actions-tabs clearfix">
                            <li id="wsu-search-tab" class="spine-search-tab closed"><button>Search</button></li>
                            <li id="wsu-contact-tab" class="spine-contact-tab closed"><button>Contact</button></li>
                            <li id="wsu-share-tab" class="spine-share-tab closed"><button>Share</button></li>
                            <li id="wsu-print-tab" class="spine-print-tab closed"><button>Print</button></li>
                        </ul>

                        <!-- Tools generated by spine.js here -->
                    </section>
                    <nav id="spine-navigation" class="spine-navigation">
                        <nav id="spine-sitenav">
                            <ul>
                                <li>
                                    <a href="~/Home/Index" class="@(ViewBag.SelectedLink == "Home" ? "current" : "")">Home</a>
                                </li>
                                <li>
                                    <a href="#" class="@(ViewBag.SelectedLink == "Search" ? "current" : "")">Searches</a>
                                    <ul>
                                        <li><a href="~/Search/Quick" class="@(ViewBag.SelectedLink == "Search.Quick" ? "current" : "")">Quick</a></li>
                                        <li><a href="~/Search/Advanced" class="@(ViewBag.SelectedLink == "AdvancedSearch" ? "current" : "")">Advanced</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a href="#" class="@(ViewBag.SelectedLink == "Lookup" ? "current" : "")">Lookup</a>
                                    <ul>
                                        <li><a href="~/Lookup/Applications" class="@(ViewBag.SelectedLink == "Lookup.Applications" ? "current" : "")">Application</a></li>
                                        <li><a href="~/Lookup/Crops" class="@(ViewBag.SelectedLink == "Lookup.Crops" ? "current" : "")">Crop</a></li>
                                        <li><a href="~/Lookup/Formulations" class="@(ViewBag.SelectedLink == "Lookup.Formulations" ? "current" : "")">Formulation</a></li>
                                        <li><a href="~/Lookup/Ingredients" class="@(ViewBag.SelectedLink == "Lookup.Ingredients" ? "current" : "")">Ingredient</a></li>
                                        <li><a href="~/Lookup/IntendedUsers" class="@(ViewBag.SelectedLink == "Lookup.IntendedUsers" ? "current" : "")">Intended User</a></li>
                                        <li><a href="~/Lookup/Pests" class="@(ViewBag.SelectedLink == "Lookup.Pests" ? "current" : "")">Pest</a></li>
                                        <li><a href="~/Lookup/PesticideTypes" class="@(ViewBag.SelectedLink == "Lookup.PesticideTypes" ? "current" : "")">Pesticide Type</a></li>
                                        <li><a href="~/Lookup/Registrants" class="@(ViewBag.SelectedLink == "Lookup.Registrants" ? "current" : "")">Registrant</a></li>
                                        <li><a href="~/Lookup/Resistance" class="@(ViewBag.SelectedLink == "Lookup.Resistance" ? "current" : "")">Resistance</a></li>
                                        <li><a href="~/Lookup/SignalWords" class="@(ViewBag.SelectedLink == "Lookup.SignalWords" ? "current" : "")">Signal Word</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a href="#" class="@(ViewBag.SelectedLink == "NewAccount.Index" ? "current" : "")">Account</a>
                                    @{
                                        if (Request.Cookies[Convert.ToString(System.Web.Configuration.WebConfigurationManager.AppSettings["ApplicationName"]) + ".Authentication"] == null)
                                        {
                                            <ul>
                                                <li><a href="~/Session/Create" class="@(ViewBag.SelectedLink == "Session.Create" ? "current" : "")">Login</a></li>
                                            </ul>
                                            <ul>
                                                <li><a href="~/NewAccount/Create" class="@(ViewBag.SelectedLink == "NewAccount.Create" ? "current" : "")">Create</a></li>
                                            </ul>
                                        }
                                        else
                                        {
                                            <ul>
                                                <li><a href="~/Account/Details" class="@(ViewBag.SelectedLink == "Account.Details" ? "current" : "")">Details</a></li>
                                            </ul>
                                            <ul>
                                                <li><a href="~/Account/ResetPassword" class="@(ViewBag.SelectedLink == "Account.ResetPassword" ? "current" : "")">Reset Password</a></li>
                                            </ul>
                                            <ul>
                                                <li><a href="~/Account/UpdateEmail" class="@(ViewBag.SelectedLink == "Account.UpdateEmail" ? "current" : "")">Update Email</a></li>
                                            </ul>
                                            <ul>
                                                <li><a href="~/Account/Activities" class="@(ViewBag.SelectedLink == "Account.Activities" ? "current" : "")">Activities</a></li>
                                            </ul>
                                            <ul>
                                                <li><a href="~/Account/RequestApiKey" class="@(ViewBag.SelectedLink == "Account.RequestApiKey" ? "current" : "")">Request API key</a></li>
                                            </ul>
                                            <ul>
                                                <li><a href="~/Session/Destroy">Logout</a></li>
                                            </ul>
                                        }
                                    }
                                </li>
                                <li><a href="~/Help/Index" class="@(ViewBag.SelectedLink == "Help.Index" ? "current" : "")">API Documentation</a></li>

                                @{
                                    if (Request.Cookies[Convert.ToString(System.Web.Configuration.WebConfigurationManager.AppSettings["ApplicationName"]) + ".Authorization"] != null)
                                    {
                                        if (Convert.ToBoolean(Picol.Classes.Encryption.UnprotectString(Request.Cookies[Convert.ToString(System.Web.Configuration.WebConfigurationManager.AppSettings["ApplicationName"]) + ".Authorization"]["Admin"], new string[] { "Cookie" }, true)))
                                        {
                                            <li>
                                                <a href="#">Admin</a>
                                                <ul>
                                                    <li><a href="~/Admin/ListApiKeys" class="@(ViewBag.SelectedLink == "Admin.ApiKeys" ? "current" : "")">API Keys</a></li>
                                                    <li><a href="~/Application/List" class="@(ViewBag.SelectedLink == "Application.List" ? "current" : "")">Application</a></li>
                                                    <li><a href="~/Crop/List" class="@(ViewBag.SelectedLink == "Crop.List" ? "current" : "")">Crop</a></li>
                                                    <li><a href="~/CropGroup/List" class="@(ViewBag.SelectedLink == "CropGroup.List" ? "current" : "")">Crop Group</a></li>
                                                    <li><a href="~/Admin/DataLoad" class="@(ViewBag.SelectedLink == "Admin.DataLoad" ? "current" : "")">Data Load</a></li>
                                                    <li><a href="~/Admin/EditApiTos" class="@(ViewBag.SelectedLink == "Admin.EditApiTos" ? "current" : "")">Edit API ToS</a></li>
                                                    <li><a href="~/Admin/EditApiUse" class="@(ViewBag.SelectedLink == "Admin.EditApiUse" ? "current" : "")">Edit API Use</a></li>
                                                    <li><a href="~/Admin/EditFaq" class="@(ViewBag.SelectedLink == "Admin.EditFaq" ? "current" : "")">Edit FAQ</a></li>
                                                    <li><a href="~/Admin/EditHome" class="@(ViewBag.SelectedLink == "Admin.EditHome" ? "current" : "")">Edit Home</a></li>
                                                    <li><a href="~/Admin/EditVideoTutorials" class="@(ViewBag.SelectedLink == "Admin.EditVideoTutorials" ? "current" : "")">Edit Video Tutorials</a></li>
                                                    <li><a href="~/Formulation/List" class="@(ViewBag.SelectedLink == "Formulation.List" ? "current" : "")">Formulation</a></li>
                                                    <li><a href="~/Ingredient/List" class="@(ViewBag.SelectedLink == "Ingredient.List" ? "current" : "")">Ingredient</a></li>
                                                    <li>
                                                        <a href="#">Upload Processing</a>
                                                        <ul>
                                                            <li>
                                                                <a href="~/CertificateProcessing/Uploaded" class="@(ViewBag.SelectedLink == "CertificateProcessing.Uploaded" ? "current" : "")">Process Certificates</a>
                                                            </li>
                                                            <li>
                                                                <a href="~/CertificateProcessing/InTransit" class="@(ViewBag.SelectedLink == "CertificateProcessing.InTransit" ? "current" : "")">In Transit</a>
                                                            </li>
                                                            <li>
                                                                <a href="~/CertificateProcessing/Reviewed" class="@(ViewBag.SelectedLink == "CertificateProcessing.Reviewed" ? "current" : "")">Reviewed</a>
                                                            </li>
                                                        </ul>
                                                    </li>
                                                    <li><a href="~/Label/List" class="@(ViewBag.SelectedLink == "Label.List" ? "current" : "")">Label</a></li>
                                                    <li><a href="~/Label/Create" class="@(ViewBag.SelectedLink == "Label.Create" ? "current" : "")">Create Label</a></li>
                                                    <li><a href="~/Label/Upload" class="@(ViewBag.SelectedLink == "Label.Upload" ? "current" : "")">Process Label</a></li>
                                                    <li><a href="~/Pest/List" class="@(ViewBag.SelectedLink == "Pest.List" ? "current" : "")">Pest</a></li>
                                                    <li><a href="~/Registrant/List" class="@(ViewBag.SelectedLink == "Registrant.List" ? "current" : "")">Registrant</a></li>
                                                    <li><a href="~/RegulationAuthority/List" class="@(ViewBag.SelectedLink == "RegulationAuthority.List" ? "current" : "")">Regulation Authority</a></li>
                                                    <li><a href="~/Resistance/List" class="@(ViewBag.SelectedLink == "Resistance.List" ? "current" : "")">Resistance</a></li>
                                                    <li><a href="~/Admin/ListSettings" class="@(ViewBag.SelectedLink == "Admin.Settings" ? "current" : "")">Settings</a></li>
                                                    <li><a href="~/SignalWord/List" class="@(ViewBag.SelectedLink == "SignalWord.List" ? "current" : "")">Signal Word</a></li>
                                                    <li><a href="~/ToleranceCrop/List" class="@(ViewBag.SelectedLink == "ToleranceCrop.List" ? "current" : "")">Tolerance Crop</a></li>
                                                    <li><a href="~/ToleranceType/List" class="@(ViewBag.SelectedLink == "ToleranceType.List" ? "current" : "")">Tolerance Type</a></li>
                                                    <li><a href="~/Usage/List" class="@(ViewBag.SelectedLink == "Usage.List" ? "current" : "")">Usage</a></li>
                                                    <li><a href="~/User/List" class="@(ViewBag.SelectedLink == "User.List" ? "current" : "")">User</a></li>
                                                </ul>
                                            </li>
                                        }
                                    }
                                }

                                <li>
                                    <a href="#" class="@(ViewBag.SelectedLink == "Help" ? "current" : "")">Help</a>
                                    <ul>
                                        <li><a href="~/Support/Faq" class="@(ViewBag.SelectedLink == "Help.Faq" ? "current" : "")">FAQ</a></li>
                                        <li><a href="~/Support/VideoTutorials" class="@(ViewBag.SelectedLink == "Help.VideoTutorials" ? "current" : "")">Video Tutorials</a></li>
                                        <li><a href="https://agr.wa.gov/FP/Pubs/docs/398-WSDACriteriaForPesticideUseOnMarijuana.pdf" class="@(ViewBag.SelectedLink == "Help" ? "current" : "")" target="_blank">I-502/I-692 Criteria</a></li>
                                        <li><a href="https://agr.wa.gov/FP/Pubs/docs/567-CriteriaPesticideUseOnIndustHemp.pdf" class="@(ViewBag.SelectedLink == "Help" ? "current" : "")" target="_blank">ESSB-6206 Criteria</a></li>
                                    </ul>
                                </li>
                                <li><a href="~/Home/PrivacyPolicy" class="@(ViewBag.SelectedLink == "PrivacyPolicy" ? "current" : "")">Privacy Policy</a></li>
                            </ul>

                        </nav>

                        <nav class="spine-offsitenav">
                            <ul>
                                <li><a href="http://cwr.wsu.edu/">CWR Home</a></li>
                                <li><a href="http://cahnrs.wsu.edu/">CAHNRS Home</a></li>
                            </ul>
                        </nav>
                        <br />
                        <nav id="SearchParameterContainer" style="display:none;">
                            <ul id="SearchParameters">
                                <li style="border-width:0;">Search Parameters</li>
                            </ul>
                        </nav>
                        <label class="switch" style="display:none;" id="SearchParameterTruncateContainer">
                            <input type="checkbox" id="SearchParameterTruncateSwitch" style="transform:scale(1) !important">
                            <span class="slider round"></span>
                        </label>
                    </nav>

                    <!-- #navigation -->
                    <!-- Social and Global Links -->
                    <footer class="spine-footer">
                        <nav id="wsu-social-channels" class="spine-social-channels">
                            <ul>
                                <li class="facebook-channel"><a href="http://facebook.com/WSU.WSPRS">Facebook</a></li>
                                <li class="twitter-channel"><a href="http://twitter.com/PICOL_mgr">Twitter</a></li>
                                <li class="youtube-channel"><a href="http://www.youtube.com/channel/UClYksSQRSKbRD3_L_iV9Czw">YouTube</a></li>
                                <li class="directory-channel"><a href="http://social.wsu.edu">Directory</a></li>
                            </ul>
                        </nav>
                        <nav id="wsu-global-links" class="spine-global-links">
                            <ul>
                                <li class="zzusis-link"><a href="http://zzusis.wsu.edu/">Zzusis</a></li>
                                <li class="access-link"><a href="http://access.wsu.edu/">Access</a></li>
                                <li class="policies-link"><a href="http://policies.wsu.edu/">Policies</a></li>
                                <li class="copyright-link"><a href="http://copyright.wsu.edu">&copy;</a></li>
                            </ul>
                        </nav>
                    </footer>

                </div><!--/glue-->
            </div><!--/spine-->
        </div><!--/cover-->
    </div><!--/jacket-->

    <div id="contact-details" itemscope itemtype="http://schema.org/Organization">
        <meta itemprop="department" content="WSPRS"><!-- required -->
        <meta itemprop="name" content="Washington State University"><!-- required -->
        <div itemprop="address" itemscope itemtype="http://schema.org/PostalAddress">
            <meta itemprop="location" content="Kalkus Hall"><!-- optional -->
            <meta itemprop="streetAddress" content="2606 West Pioneer"><!-- required -->
            <meta itemprop="addressLocality" content="Puyallup"><!-- required -->
            <meta itemprop="addressRegion" content="WA"><!-- optional -->
            <meta itemprop="postalCode" content="98371"><!-- required -->
        </div>
        <meta itemprop="logo" content="/Content/images/cahnrsLogo.png"><!-- optional -->
        <meta itemprop="telephone" content="(253) 445-4517"><!-- required -->
        <meta itemprop="email" content="picol.info@wsu.edu"><!-- required -->
        <meta itemprop="contactPoint" content="Lenora Jones"><!-- optional -->
        <meta itemprop="url" content="#"><!-- optional -->
    </div>
</body>
</html>